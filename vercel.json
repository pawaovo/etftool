{
  "routes": [
    { "src": "/", "dest": "/splan.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "outputDirectory": "dist",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ]
} 